{
    "name": "@miitvip/captcha",
    "version": "1.0.0",
    "private": false,
    "type": "module",
    "main": "dist/lib/makeit-captcha.cjs.js",
    "module": "dist/es/makeit-captcha.es.js",
    "unpkg": "dist/makeit-captcha.min.js",
    "types": "./dist/es/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/es/index.d.ts",
            "import": "./dist/es/makeit-captcha.es.js",
            "require": "./dist/lib/makeit-captcha.cjs.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "dev": "vite",
        "clean": "rimraf dist",
        "build": "npm run clean && npm run build:base && npm run build:iife",
        "build:base": "vite build --config vite.config.base.ts",
        "build:iife": "vite build --config vite.config.iife.ts",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build tsconfig.app.json",
        "lint": "eslint . --fix",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "ant-design-vue": "^4.2.6",
        "axios": "^1.10.0",
        "vue": "^3.5.17",
        "vue-i18n": "^11.1.9"
    },
    "devDependencies": {
        "@babel/core": "^7.28.0",
        "@babel/preset-env": "^7.28.0",
        "@rollup/plugin-babel": "^6.0.4",
        "@tsconfig/node22": "^22.0.2",
        "@types/babel__core": "^7.20.5",
        "@types/node": "^22.13.14",
        "@vitejs/plugin-vue": "^6.0.0",
        "@vitejs/plugin-vue-jsx": "^5.0.0",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.1",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.29.0",
        "eslint-plugin-vue": "~10.2.0",
        "jiti": "^2.4.2",
        "less": "^4.3.0",
        "npm-run-all2": "^8.0.4",
        "prettier": "3.5.3",
        "rimraf": "^6.0.1",
        "rollup-plugin-dts": "^6.2.1",
        "typescript": "~5.8.0",
        "vite": "^6.3.5",
        "vite-plugin-dts": "^4.5.4",
        "vite-plugin-vue-devtools": "^7.7.7",
        "vue-tsc": "^2.2.10"
    },
    "engines": {
        "node": ">=20.x.x"
    }
